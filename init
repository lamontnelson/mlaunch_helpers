NODES=`cat NODES`
VERSION=`cat VERSION`
PORT=`cat PORT`
MONGODB_BIN=`m bin $VERSION`
NUM_SHARDS=`cat SHARDS`
SHARDS=""
for i in `seq 1 $NUM_SHARDS` 
do
  SHARDS="$SHARDS sh$i"
done
mlaunch init --replicaset --port $PORT --nodes $NODES --binarypath $MONGODB_BIN --hostname localhost --mongos 1 --sharded $SHARDS 

